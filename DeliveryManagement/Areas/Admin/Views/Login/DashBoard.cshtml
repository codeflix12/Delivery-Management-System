</head>


<html>
<body>


 <!--<h2>Hello</h2>
<fieldset>  
    <legend> User DashBoard </legend>  
  
// </fieldset>-->  



        <!-- Left Panel -->
    @{Html.RenderAction("SideNav","Include");}
<!-- /#left-panel -->

    <!-- Left Panel -->

    <!-- Right Panel -->

    <div id="right-panel" class="right-panel">

        <!-- Header-->
        @{Html.RenderAction("Header", "Include");}
       <!-- /header -->
        <!-- Header-->
        

        <div class="content mt-3">
            <div class="animated fadeIn">
                <div class="row">

               
                  @using (Html.BeginForm())
                  { }
                      <section role="main" class="content-body">
					<header class="page-header">
						<h2>Delivery Status</h2>
					
						<div class="right-wrapper pull-right">
							<ol class="breadcrumbs">
								<li>
									<a href="index.html">
										<i class="fa fa-home"></i>
									</a>
								</li>
								<li><span>Tables</span></li>
								<li><span>Status</span></li>
							</ol>
					
							<a class="sidebar-right-toggle" data-open="sidebar-right"><i class="fa fa-chevron-left"></i></a>
						</div>
					</header>

					<!-- start: page -->
						<section class="panel">
							<header class="panel-heading">
								<div class="panel-actions">
									<a href="#" class="fa fa-caret-down"></a>
									<a href="#" class="fa fa-times"></a>
								</div>
						
								<h2 class="panel-title">Status Table</h2>
							</header>
							<div class="panel-body">
								<table class="table table-bordered table-striped mb-none" id="datatable-tabletools" data-swf-path="~Content/Admin/vendor/jquery-datatables/extras/TableTools/swf/copy_csv_xls_pdf.swf">
									<thead>
						
                      <tr class="gradeX">
                        <th>Sr. No.</th>
                        <th>Order Number</th>
                        <th>Mobile Number</th>
                        <th>Order Delivery</th>
                        <th>Going for PickUp</th>
                        <th>Reached PickUp</th>
                        <th>PickedUp</th>
                        <th>Going For Delivery</th>
                        <th>Delivery</th>
                        <th>Action</th>
                      </tr>
                    </thead>
                    <tbody>
                        
                        @foreach (var driver in Model)
                        {
                      <tr class="gradeX">
                        <td>@{ i++; }
                            @i</td>
                        <td>@driver.orderno</td>
                        <td>@driver.mobileno</td>
                        <td>@driver.orderno</td>
                        <td>@driver.orderno</td>
                        <td>@driver.orderno</td>
                        <td>@driver.deliverycharge</td>
                        <td>@driver.deliverydate</td>
                        <td>@Html.ActionLink("Edit", "Edit", new {id = driver.id})</td>
                        <td>@Html.ActionLink("Delete","Delete", new {id = driver.id})</td>
                      </tr>
                       
                        }
                      
                    </tbody>
                  </table>
                        </div>
                            </section>
                          </section>
                    </div>
                </div>
            

                </div>
          @*  </div><!-- .animated -->
    
        </div><!-- .content -->


    </div>*@<!-- /#right-panel -->

    <!-- Right Panel -->
    @{Html.RenderAction("Footer","Include");}

